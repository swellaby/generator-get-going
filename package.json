{
    "name": "generator-lets-go",
    "description": "Yeoman generator for Golang projects",
    "version": "0.0.1",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/swellaby/generator-lets-go.git"
    },
    "author": {
        "url": "http://swellaby.com",
        "name": "Swellaby",
        "email": "opensource@swellaby.com"
    },
    "contributors": [
        {
            "email": "opensource@swellaby.com",
            "name": "Caleb Cartwright",
            "url": "https://github.com/calebcartwright"
        }
    ],
    "bugs": {
        "url": "https://github.com/swellaby/generator-lets-go/issues"
    },
    "scripts": {
        "build": "npm run coverage:enforce && npm run lint:build",
        "clean:test": "rimraf .testresults .coverage",
        "clean:test:unit": "rimraf .testresults/unit .coverage/unit",
        "clean:test:component": "rimraf .testresults/component .coverage/component",
        "clean:js": "rimraf test/**/{*.js,*.map} generators/{app/*.js*,*.js*}",
        "clean": "npm run clean:test && npm run clean:js",
        "pretranspile": "npm run clean:js",
        "transpile": "tsc -p tsconfig.json",
        "tslint": "tslint -p tsconfig.json -c tslint.json -t stylish generators/**/*.ts test/**/*.ts",
        "eslint": "eslint --no-eslintrc -c .eslintrc.js **/*.js",
        "prelint": "npm run transpile",
        "lint": "npm run tslint && npm run eslint",
        "lint:build": "npm run tslint && npm run eslint",
        "test": "npm run test:unit",
        "pretest:unit": "npm run transpile && npm run clean:test:unit",
        "test:unit": "mocha --opts test/unit/mocha.opts",
        "posttest:unit": "replace-in-file --isRegex true \"/(.js)/g\" .ts .testresults/unit/sonar.xml",
        "pretest:component": "npm run transpile && npm run clean:test:component",
        "test:component": "mocha --opts test/component/mocha.opts",
        "coverage": "nyc --report-dir .coverage/unit --check-coverage false npm run test:unit",
        "coverage:enforce": "nyc --report-dir .coverage/unit npm run test:unit",
        "coverage:open": "npm run coverage && opn .coverage/unit/index.html",
        "coverage:component": "nyc --report-dir .coverage/component --check-coverage false npm run test:component",
        "coverage:component:enforce": "nyc --report-dir .coverage/component npm run test:component",
        "coverage:component:open": "npm run coverage:component && opn .coverage/component/index.html",
        "dev:reset": "npm run clean && node -e \"require('rimraf').sync('node_modules');\" && npm run dev:setup",
        "dev:setup": "npm install && npm i -g yo && npm run build"
    },
    "devDependencies": {
        "@swellaby/eslint-config": "^1.1.6",
        "@swellaby/tslint-config": "^1.1.5",
        "@types/chai": "^4.1.5",
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.11.0",
        "@types/sinon": "^5.0.2",
        "@types/yeoman-generator": "^2.0.3",
        "@types/yosay": "^0.0.29",
        "chai": "^4.1.2",
        "eslint": "^5.6.0",
        "husky": "^1.0.0-rc.15",
        "mocha": "^5.2.0",
        "mocha-multi-reporters": "^1.1.7",
        "mocha-sonarqube-reporter": "^1.0.1",
        "nyc": "^13.0.1",
        "opn-cli": "^3.1.0",
        "replace-in-file": "^3.4.2",
        "rimraf": "^2.6.2",
        "sinon": "^6.3.4",
        "tslint": "^5.11.0",
        "typescript": "^3.0.3"
    },
    "dependencies": {
        "tslib": "^1.9.3",
        "yeoman-generator": "^3.1.1",
        "yosay": "^2.0.2"
    }
}
